<Window x:Class="Jiyu_Manager.DebugOutput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        xmlns:local="clr-namespace:Jiyu_Manager"
        ui:WindowHelper.UseModernWindowStyle="True"
        mc:Ignorable="d"
        Title="DebugOutput" Height="300" Width="436">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal" Margin="10,10,0,0">
                <Label Content="For Debug Use" HorizontalAlignment="Left" Height="25" Margin="0,0,0,0" VerticalAlignment="Top" Width="132" FontFamily="Consolas" FontSize="18"/>
                <Button Content="导出至txt" HorizontalAlignment="Left" Height="25" Margin="0,0,0,0" VerticalAlignment="Top" Width="76" FontSize="10"/>
            </StackPanel>
            <TextBox x:Name="DebugOutputTextbox" Margin="10,10,10,10" IsReadOnly="True" HorizontalScrollBarVisibility="Auto" Height="210"/>
        </StackPanel>
        

    </Grid>
</Window>
